{
  "name": "embroidery-essential",
  "version": "1.0.0",
  "description": "Embroidery Essential",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,css,html}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "dev": "rm -rf dist/* && parcel src/index.html",
    "build-dev": "rm -rf dist/* && parcel build src/index.html",
    "build-github": "rm -rf docs/* && parcel build src/index.html --public-url /"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edworczak/embroidery-essential.git"
  },
  "author": "Ewa Dworczak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edworczak/embroidery-essential/issues"
  },
  "homepage": "http://edworczak.github.io/embroidery-essential",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.12.13",
    "@reduxjs/toolkit": "^1.9.3",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.32.2",
    "parcel": "^2.8.3",
    "prettier": "^2.8.1",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "react-redux": "^8.0.5",
    "sharp": "^0.31.3",
    "typescript": "^5.4.5",
    "url": "^0.11.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.10.0",
    "sort-by": "^1.2.0",
    "styled-components": "^5.3.3"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "web": "./docs/index.html",
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./"
    }
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
